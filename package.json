  "dependencies": {
    "better-sqlite3": "^7.5.0",
    "electron-log": "^4.4.3",
    "fast-csv": "^4.3.6",
    "fuse.js": "^6.5.3",
    "mongoose": "^6.1.6",
    "slash": "^3.0.0",
    "sqlite3": "^5.0.2"
  }
